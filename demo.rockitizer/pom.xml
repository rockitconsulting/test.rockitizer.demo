	<groupId>rockit</groupId>
	<artifactId>demo.rockitizer</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<!--packaging>jar</packaging -->
	<profiles>
		<profile>
			<id>demo</id>
			<properties>
				<env>demo</env>
			</properties>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
		</profile>
	</profiles>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
		</plugins>
